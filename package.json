{
  "name": "rsr-demo",
  "version": "0.0.1-rc.0",
  "scripts": {
    "dev": "modern dev",
    "build": "modern build",
    "start": "modern start",
    "serve": "modern serve",
    "new": "modern new",
    "upgrade": "modern upgrade",
    "lint": "biome check",
    "format": "biome format --write",
    "reset": "npx rimraf node_modules ./**/node_modules",
    "prepare": "simple-git-hooks"
  },
  "engines": {
    "node": ">=16.18.1"
  },
  "lint-staged": {
    "*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
      "biome check --files-ignore-unknown=true"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "dependencies": {
    "@lanaqi/rsr": "0.0.1-rc.0",
    "@lanaqi/rsr-nprogress": "0.0.1-rc.0",
    "@modern-js/plugin-tailwindcss": "^2.65.5",
    "@modern-js/runtime": "^2.65.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@modern-js/app-tools": "^2.65.5",
    "@modern-js/tsconfig": "^2.65.5",
    "@types/jest": "~29.2.6",
    "@types/node": "~18.11.19",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.5",
    "lint-staged": "~15.4.3",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.12.1",
    "tailwindcss": "~3.4.17",
    "typescript": "~5.7.3"
  }
}
